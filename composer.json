{
    "name": "guidocella/eloquent-populator",
    "type": "library",
    "description": "Automatically populate Eloquent models",
    "keywords": ["laravel", "eloquent", "database", "faker"],
    "license": "MIT",
    "require": {
        "php": ">=5.6.4",
        "illuminate/database": "5.3.*|5.4.*|5.5.*|5.6.*",
        "fzaninotto/faker": "^1.4",
        "doctrine/dbal": "^2.5"
    },
    "require-dev": {
        "laravel/laravel": "5.6.*",
        "dimsav/laravel-translatable": "^6",
        "phpunit/phpunit": "^6",
        "themsaid/laravel-multilingual": "^1"
    },
    "autoload": {
        "psr-4": {
            "EloquentPopulator\\": "src"
        },
        "files": [
            "src/helper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "EloquentPopulator\\": "tests"
        }
    }
}
